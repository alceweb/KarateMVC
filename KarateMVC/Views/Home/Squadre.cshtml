@model IEnumerable<KarateMVC.Models.RoleViewModel>

@{
    ViewBag.Title = "Squadre";
}

<h2>@ViewBag.Title</h2>
<table class="table table-hover">
    @foreach (var item in ViewBag.Roles)
            {
        <tr>
            <td>
                <strong><em>@item.Name</em></strong>@item
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id, usr = item.Name })
            </td>
            <td></td>
        </tr>
    }
</table>


